
@{
    ViewBag.Title = "AddProject";
    Layout = "~/Views/Shared/_PortfolioAdminLayout.cshtml";
}

<h2>Yeni Proje  Ekleme Sayfası</h2>
<form method="post">
    <label>Proje Adı  </label>
    <input type="text" class="form-control" name="ProjectName" />
    <br />
    <label>Proje Açıklaması</label>
    <input type="text" class="form-control" name="ProjectDescription" />
    <br />
    <label>Proje Tarihi</label>
    <input type="text" class="form-control" name="ProjectDate" />
    <br />
    <label>Proje Türü</label>
    <input type="text" class="form-control" name="ProjectType" />
    <br />
    <label>Kullanılan Teknolojiler</label>
    <input type="text" class="form-control" name="TechnologiesUsed" />
    <br />
    <button class="btn btn-info">Proje   Kaydet</button>
</form>




